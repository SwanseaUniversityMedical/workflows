{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    ":enableRenovate"
  ],
  "schedule": [
    "* * * * *"
  ],
  "packageRules": [
    {
      "matchPackageNames": [
        "SwanseaUniversityMedical/workflows"
      ],
      "enabled": true,
      "pinDigests": false,
      "additionalBranchPrefix": "{{{compatibility}}}-",
      "commitMessageAction": "{{{compatibility}}} - update",
      "versioning": "regex:^v(?<major>\\d+)\\.(?<minor>\\d+)\\.(?<patch>\\d+)-(?<compatibility>.+)$"
    }
  ]
}
